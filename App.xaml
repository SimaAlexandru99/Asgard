<Application x:Class="Asgard.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
			 xmlns:local="clr-namespace:Asgard"
			 xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
			 StartupUri="/Insider/Pages/SignIn.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/LightTheme.xaml" />
				<ResourceDictionary Source="Styles/CustomWindowStyle.xaml" />
				<ResourceDictionary Source="Styles/Styles.xaml" />
				<ResourceDictionary Source="Styles/InsiderStyles.xaml" />
				<materialDesign:CustomColorTheme BaseTheme="Dark" PrimaryColor="#007FFF" SecondaryColor="White" />
				<materialDesign:CustomColorTheme BaseTheme="Light" PrimaryColor="#007FFF" SecondaryColor="Black" />
				<ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
