			<StackPanel Grid.Row="25" Grid.Column="0">
				<TextBlock Text="Observatii generale." Foreground="{DynamicResource TextColor}" FontFamily="/Fonts/#Rubik-Bold" FontSize="18" Margin="0,30,0,10" FontWeight="Bold"/>

